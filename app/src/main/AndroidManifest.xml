<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.prac.mvp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:name="com.prac.mvp.android.LearnApplicationImpl"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.prac.mvp.android.activity.view.ResultActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.prac.mvp.android.activity.view.AlbumDetailActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.prac.mvp.android.activity.view.ArtistDetailActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.prac.mvp.android.activity.view.TrackDetailActivity" android:screenOrientation="portrait"/>

    </application>


</manifest>